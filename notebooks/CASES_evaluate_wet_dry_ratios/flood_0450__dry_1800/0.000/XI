FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      XI;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 0.000;

boundaryField
{
front
    {
        type    empty;
    }

    back
    {
        type    empty;
    }

    right
    {
        type    empty;
    }

    left
    {
        type    empty;
    }

    top
    {
        type            zeroGradient;
    }

    bottom
    {
        type            zeroGradient;
    }
}

