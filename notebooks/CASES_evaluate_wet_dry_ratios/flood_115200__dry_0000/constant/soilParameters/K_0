FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.000";
    object      K_0;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

/*
    See Table 4.1 from Rosenzweig thesis (2011)
    For Caesarea sand without added xantan, van Genuchten parameters are:
        thera_r = 0.012
        theta_s = 0.385
        n = 7.26
        alpha = 0.0279 1/cm
    
    From Ch.8 (p. 132)
        Ks = 1.24 cm/min
*/

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform 2.067E-4; 

boundaryField
{
    top
    {
        type            zeroGradient;
    }
    left
    {
        type            empty;
    }
    right
    {
        type            empty;
    }
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
    bottom
    {
        type            zeroGradient;
    }
}


