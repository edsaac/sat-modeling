FoamFile
{
    version         2;
    format          ascii;
    class           dictionary;
    location        "system";
    object          controlDict;
}

application     unsatRespiration;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         6.912e+06;

writeControl    runTime;

writeInterval   17280;

deltaT          0.001;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      fixed;

timePrecision   3;

runTimeModifiable true;

adjustTimeStep  yes;

maxCo           1;

debug           1;

functions
{
#includeFunc boundaryProbes
}


